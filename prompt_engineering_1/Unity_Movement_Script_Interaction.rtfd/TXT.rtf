using System.Collections;\
using System.Collections.Generic;\
using UnityEngine;\
\
public class PlayerMovement : MonoBehaviour\
\{\
    // Movement variables\
    public float walkSpeed = 5f;\
    public float runSpeed = 8f;\
    public float friction = 10f;\
    private float currentSpeed = 0f; // Current speed that is either walkSpeed or runSpeed\
    private Vector2 movementInput;\
\
    private Rigidbody2D rb;\
\
    void Start()\
    \{\
        rb = GetComponent<Rigidbody2D>();\
    \}\
\
    void Update()\
    \{\
        // Get horizontal input (left/right movement)\
        float moveX = Input.GetAxisRaw("Horizontal");\
\
        // Determine if running or walking\
        if (Input.GetKey(KeyCode.LeftShift))  // If Left Shift is held down\
        \{\
            currentSpeed = runSpeed;  // Run\
        \}\
        else\
        \{\
            currentSpeed = walkSpeed;  // Walk\
        \}\
\
        // Get normalized movement input\
        movementInput = new Vector2(moveX, 0f).normalized;\
\
        // Apply friction (slows down after no input)\
        if (moveX == 0)\
        \{\
            rb.velocity = new Vector2(Mathf.Lerp(rb.velocity.x, 0, friction * Time.deltaTime), rb.velocity.y);\
        \}\
    \}\
\
    void FixedUpdate()\
    \{\
        // Apply movement to Rigidbody2D\
        rb.velocity = new Vector2(movementInput.x * currentSpeed, rb.velocity.y);\
    \}\
\}\

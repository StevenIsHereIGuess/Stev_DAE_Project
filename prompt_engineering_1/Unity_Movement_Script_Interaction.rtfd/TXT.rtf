using System.Collections;\
using System.Collections.Generic;\
using UnityEngine;\
\
public class PlayerMovement : MonoBehaviour\
\{\
    // Variables for movement\
    public float walkSpeed = 5f; // Walking speed\
    public float runSpeed = 8f;  // Running speed\
    public float friction = 10f; // How fast the player slows down after stopping\
    private float moveSpeed = 0f; // The current speed of the player\
    private Vector2 movementInput;\
\
    private Rigidbody2D rb;\
\
    void Start()\
    \{\
        // Get the Rigidbody2D component\
        rb = GetComponent<Rigidbody2D>();\
    \}\
\
    void Update()\
    \{\
        // Get player input\
        float moveX = Input.GetAxisRaw("Horizontal");\
\
        // Determine if the player is walking or running\
        if (Input.GetKey(KeyCode.LeftShift))\
        \{\
            moveSpeed = runSpeed;\
        \}\
        else\
        \{\
            moveSpeed = walkSpeed;\
        \}\
\
        // Apply movement input\
        movementInput = new Vector2(moveX, 0f).normalized;\
\
        // Handle friction (gradual deceleration when no input is given)\
        if (moveX == 0)\
        \{\
            rb.velocity = new Vector2(Mathf.Lerp(rb.velocity.x, 0, friction * Time.deltaTime), rb.velocity.y);\
        \}\
    \}\
\
    void FixedUpdate()\
    \{\
        // Apply the movement to the Rigidbody2D\
        rb.velocity = new Vector2(movementInput.x * moveSpeed, rb.velocity.y);\
    \}\
\}\
